{
  "name": "ClipItToLSF",
  "version": "0.1.17",
  "description": "LUL",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "https://*.twitch.tv/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": ["https://clips.twitch.tv/create"],
    "js": ["publishClip.js"],
    "run_at": "document_start"
  }
]
}
